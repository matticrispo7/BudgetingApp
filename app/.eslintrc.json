{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "plugins": ["react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-unused-vars": "error",
    "no-console": "off",
    "no-var": "error",
    "react/react-in-jsx-scope": "off",
    "no-duplicate-imports": "error",
    "react/prop-types": "off",
    "react/no-unescaped-entities": "off"
  }
}
